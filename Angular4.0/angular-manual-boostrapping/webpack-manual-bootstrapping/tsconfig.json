{
"compilerOptions": {
    "target":"es5",
    "module": "commonjs",
    "strict": false,
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
        "es2015",
        "dom"
    ],
    "noImplicitAny": true,
"noStrictGenericChecks": true,
"skipLibCheck": true,
"typeRoots": [
    "node_module/@types"
]
}   
}